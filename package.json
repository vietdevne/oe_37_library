        "laravel-mix": "^5.0.7",
        "lodash": "^4.17.4",
        "popper.js": "^1.16.1",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.27.0",
        "sass-loader": "^8.0.2",
        "sweetalert": "^2.1.2",
        "vue": "^2.5.7",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.15.1",
        "sweet-alert": "^2.0.5",
        "sweetalert2": "^10.8.1"
    }
